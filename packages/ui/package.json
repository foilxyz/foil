    "jsbi": "3",
    "lightweight-charts": "^4.2.1",
    "lucide-react": "^0.461.0",
    "next": "^14.2.21",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
